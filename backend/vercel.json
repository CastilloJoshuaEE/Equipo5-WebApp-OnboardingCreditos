{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"]
    },
    {
      "src": "/api-docs",
      "dest": "/index.js"
    },
    {
      "src": "/api-docs/swagger.json",
      "dest": "/index.js",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/healthz",
      "dest": "/index.js"
    },
    {
      "src": "/img/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ],
  "functions": {
    "index.js": {
      "includeFiles": "**/*.js"
    }
  }
}